#include <ostream>

template <class T>
float average (T array[], int NumElements)
{
    T tSum = T();
    
    for(int i =0; i< NumElements; i++)
    {
      tsum += array[i];
    }
    return (float)(tsum)/NumElements;
}

void main()
{
  int intArray[5] = {121,23,12,45,34,54};
  cout<< average(intArray, sizeof(intArray));
}
